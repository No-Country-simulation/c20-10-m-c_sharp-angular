#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/mario/Documents/Desarrollo/nocountry/c20-10-m-c_sharp-angular/frontend/node_modules/.pnpm/@angular+cli@18.2.1_chokidar@3.6.0/node_modules/@angular/cli/bin/node_modules:/home/mario/Documents/Desarrollo/nocountry/c20-10-m-c_sharp-angular/frontend/node_modules/.pnpm/@angular+cli@18.2.1_chokidar@3.6.0/node_modules/@angular/cli/node_modules:/home/mario/Documents/Desarrollo/nocountry/c20-10-m-c_sharp-angular/frontend/node_modules/.pnpm/@angular+cli@18.2.1_chokidar@3.6.0/node_modules/@angular/node_modules:/home/mario/Documents/Desarrollo/nocountry/c20-10-m-c_sharp-angular/frontend/node_modules/.pnpm/@angular+cli@18.2.1_chokidar@3.6.0/node_modules:/home/mario/Documents/Desarrollo/nocountry/c20-10-m-c_sharp-angular/frontend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/mario/Documents/Desarrollo/nocountry/c20-10-m-c_sharp-angular/frontend/node_modules/.pnpm/@angular+cli@18.2.1_chokidar@3.6.0/node_modules/@angular/cli/bin/node_modules:/home/mario/Documents/Desarrollo/nocountry/c20-10-m-c_sharp-angular/frontend/node_modules/.pnpm/@angular+cli@18.2.1_chokidar@3.6.0/node_modules/@angular/cli/node_modules:/home/mario/Documents/Desarrollo/nocountry/c20-10-m-c_sharp-angular/frontend/node_modules/.pnpm/@angular+cli@18.2.1_chokidar@3.6.0/node_modules/@angular/node_modules:/home/mario/Documents/Desarrollo/nocountry/c20-10-m-c_sharp-angular/frontend/node_modules/.pnpm/@angular+cli@18.2.1_chokidar@3.6.0/node_modules:/home/mario/Documents/Desarrollo/nocountry/c20-10-m-c_sharp-angular/frontend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/ng.js" "$@"
else
  exec node  "$basedir/../../bin/ng.js" "$@"
fi
