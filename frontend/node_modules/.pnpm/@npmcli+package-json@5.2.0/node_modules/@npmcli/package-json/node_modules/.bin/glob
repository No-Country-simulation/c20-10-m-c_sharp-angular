#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/mario/Documents/Desarrollo/nocountry/c20-10-m-c_sharp-angular/frontend/node_modules/.pnpm/glob@10.4.5/node_modules/glob/dist/esm/node_modules:/home/mario/Documents/Desarrollo/nocountry/c20-10-m-c_sharp-angular/frontend/node_modules/.pnpm/glob@10.4.5/node_modules/glob/dist/node_modules:/home/mario/Documents/Desarrollo/nocountry/c20-10-m-c_sharp-angular/frontend/node_modules/.pnpm/glob@10.4.5/node_modules/glob/node_modules:/home/mario/Documents/Desarrollo/nocountry/c20-10-m-c_sharp-angular/frontend/node_modules/.pnpm/glob@10.4.5/node_modules:/home/mario/Documents/Desarrollo/nocountry/c20-10-m-c_sharp-angular/frontend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/mario/Documents/Desarrollo/nocountry/c20-10-m-c_sharp-angular/frontend/node_modules/.pnpm/glob@10.4.5/node_modules/glob/dist/esm/node_modules:/home/mario/Documents/Desarrollo/nocountry/c20-10-m-c_sharp-angular/frontend/node_modules/.pnpm/glob@10.4.5/node_modules/glob/dist/node_modules:/home/mario/Documents/Desarrollo/nocountry/c20-10-m-c_sharp-angular/frontend/node_modules/.pnpm/glob@10.4.5/node_modules/glob/node_modules:/home/mario/Documents/Desarrollo/nocountry/c20-10-m-c_sharp-angular/frontend/node_modules/.pnpm/glob@10.4.5/node_modules:/home/mario/Documents/Desarrollo/nocountry/c20-10-m-c_sharp-angular/frontend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../glob@10.4.5/node_modules/glob/dist/esm/bin.mjs" "$@"
else
  exec node  "$basedir/../../../../../../glob@10.4.5/node_modules/glob/dist/esm/bin.mjs" "$@"
fi
